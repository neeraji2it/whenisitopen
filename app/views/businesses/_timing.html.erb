<div id="schedule0">
  <table cellspacing="2" cellpadding="8" style="font-size: 14px;font-weight: bold;">
    <tbody>
      <tr class="Mon">
        <td>
          <span class="day">Monday </span>
          <span class="val">
            <% if (business.mon_from == 'CLOSED' or business.mon_from == 'Closed' or business.mon_from.blank? or business.mon_to == 'Closed' or business.mon_to.blank?) %>
              <span class="border_time" id="Mon" style="position: relative;left: -20px;">
                Closed
              </span>
            <% else %>
              <span class="border_time" id="mon">
                <%= business.mon_from.split(/[:]/).first  %>
                <sup class="time_mi"><%= business.mon_from.split(/[: am pm AM PM]/).last %></sup>
              </span>
              <sup><%= business.mon_from.scan(/\d+|\D+/).last  %></sup>
              <span class="border_time" id="Mon">
                <%= business.mon_to.split(/[:]/).first  %>
                <sup class="time_mi"><%= business.mon_to.split(/[: am pm AM PM]/).last  %></sup>
              </span>
              <sup id="Mon_to"><%= business.mon_to.scan(/\d+|\D+/).last  %></sup>
            <% end %>
          </span>
        </td>
      </tr>
