      <% if business.url.present? %>
        <tr class="todaya" id="website" onclick="window.open('<%= url_with_protocol(business.url) %>', '_blank')">
          <td style="text-align: center;font-family: House Slant;font-size: 18px;cursor: pointer;font-weight: normal;">
            <%= "WEBSITE" %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
