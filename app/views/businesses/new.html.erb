<div class="con-header" style="min-height: 1050px;">
  <div class="headewrap">
    <div class="logo">
      <%= current_admin ? (link_to image_tag("/assets/about-logo.png"), imports_path) : (link_to image_tag("/assets/about-logo.png"), "/") %>
    </div>
    <%= render :partial => 'businesses/search' %>
    <span style="color: #404040;float: right;font-family: icon;font-size: 20px;font-weight: bold;left: -260px;position: relative;top: 27px;">Add new Business Information</span>
    <div class="firstResult">
      <div class="scheduleDiv" style="min-height: 100%;width: 440px;display: block;float: left;position: relative;top: 60px;left: 264px;background-color: #60BED2;padding: 20px;border-radius: 4px;">
        <div id="schedule0">
          <%= form_for @business, :url => businesses_path(), :html => {:method => 'POST', :autocomplete => 'off'} do |f| %>
            <%= render :partial => 'form', :locals => {:f => f} %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>