<div class="con-header" style="min-height: 500px;height: auto;">
  <div class="headewrap">
    <div class="logo">
      <%= link_to image_tag("/assets/about-logo.png",:width => "251", :height => "169"), imports_path %>
    </div>
    <%= render :partial => 'businesses/search' %>
  </div>
  <div style="left: 500px;position: relative;top: 150px;width: 300px;border: 1px solid black;">
    <%= form_tag upload_xls_imports_path, :multipart => true do %>
      <table cellspacing="5">
        <tr>
          <td> <b> Import the Csv data </b> </td>
        </tr>
        <tr>
          <td>
            <%= file_field_tag :file, :style=>"width: 200px;" %>
          </td>
        </tr>
        <tr>
          <td>
            <br />
            <%= submit_tag "Import", :style=>"width:75px; border-radius:20px;" %>
          </td>
        </tr>
      </table>
    <%end%>
  </div>
</div>
<script>
  $(document).ready(function() {
    $('body').removeClass('open').addClass('cont');
  })
</script>
