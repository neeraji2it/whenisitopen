<div class="con-header">
  <div class="headewrap">
    <div class="logo">
      <%= link_to image_tag("/assets/about-logo.png"), "/" %>
    </div>
  </div>
  <div style="left: 500px;position: relative;top: 150px;width: 300px;border: 1px solid black;">
    <%= form_tag upload_xls_imports_path, :multipart => true do %>
      <table cellspacing="5">
        <tr>
          <td> <b> Import the Xls data </b> </td>
        </tr>
        <tr>
          <td>
            <%= file_field_tag :file, :style=>"width: 200px;" %>
          </td>
        </tr>
        <tr>
          <td>
            <br />
            <%= submit_tag "Import", :style=>"width:75px; border-radius:20px;" %>
          </td>
        </tr>
      </table>
    <%end%>
  </div>
</div>